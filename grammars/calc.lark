start: expr

?expr: term
     | expr "+" term  -> add
     | expr "-" term  -> subtract

?term: factor
     | term "*" factor  -> multiply
     | term "/" factor  -> divide

?factor: NUMBER        -> number
       | "(" expr ")"


NUMBER: /(([0-9]+[eE][+-]?[0-9]+)|([0-9]\.[0-9]+|\.[0-9]+)([eE][+-]?[0-9]+)?)|[0-9]+/

WS: /[ \t\f\r\n]+/
%ignore WS
