start: expr

?expr: term
     | expr "+" term  -> add
     | expr "-" term  -> subtract

?term: factor
     | term "*" factor  -> multiply
     | term "/" factor  -> divide

?factor: NUMBER        -> number
       | "(" expr ")"


NUMBER: /((\d+[eE][+-]?\d+)|(\d\.\d+|\.\d+)([eE][+-]?\d+)?)|\d+/

WS: /[ \t\f\r\n]+/
%ignore WS
